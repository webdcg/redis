{
    "name": "webdcg/redis",
    "description": "Redis client for PHP using the PhpRedis C Extension",
    "keywords": [
        "webdcg",
        "redis",
        "php",
        "extension",
        "phpredis"
    ],
    "homepage": "https://github.com/webdcg/redis",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Roberto Luna Rojas",
            "email": "rluna@webdcg.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "scrutinizer/ocular": "^1.6",
        "symfony/process": "^5.0",
        "symfony/var-dumper": "^5.0",
        "symplify/easy-coding-standard": "^7.2",
        "theseer/phpdox": "^0.12.0"
    },
    "autoload": {
        "psr-4": {
            "Webdcg\\Redis\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webdcg\\Redis\\Tests\\": "tests"
        }
    },
    "scripts": {
        "check": "vendor/bin/ecs check",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
