{
    "name": "webdcg/redis",
    "description": "Object Oriented Redis",
    "keywords": [
        "webdcg",
        "redis"
    ],
    "homepage": "https://github.com/webdcg/redis",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Roberto Luna Rojas",
            "email": "rluna@webdcg.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Webdcg\\Redis\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webdcg\\Redis\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
